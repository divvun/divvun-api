server {
	root /var/www/html;
	index index.html index.htm index.nginx-debian.html;

	server_name {{ domain }};

	location / {
        proxy_set_header X-Real-IP  $remote_addr;
        proxy_set_header X-Forwarded-For $remote_addr;
        proxy_set_header Host $host;
        proxy_pass http://127.0.0.1:8080;
	}
}